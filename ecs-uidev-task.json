{
    "requiresCompatibilities": [
      "EC2"
    ],
    "containerDefinitions": [
      {
        "name": "",
        "image": "",
        "memory": 512,
        "cpu": 512,
        "essential": true,
        "portMappings": [
            {
              "hostPort": 0,
              "protocol": "tcp",
              "containerPort": 3000
            }
        ],
        "mountPoints": [
            {
              "sourceVolume": "logfolder",
              "containerPath": "/prod/msp/logs"
            }
        ],
        
        "environment": [
            {
              "name": "APP_NAME",
              "value": "development"
            }
            ]
        }
        ],
    "volumes": [
        {
            "host": {
                "sourcePath": "/prod/msp/logs"
            },
            "name": "logfolder"
        }
    
    ],
    "networkMode": "bridge",
    "placementConstraints": []
  }
  
